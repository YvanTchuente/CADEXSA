<?xml version="1.0" encoding="UTF-8" ?>
<entity name="Cadexsa\Domain\Model\Event\Event" table="events">
    <field name="id" column="id"/>
    <field name="name" column="name"/>
    <field name="venue" column="venue"/>
    <field name="occursOn" column="occurs_on"/>
    <field name="description" column="description"/>
    <embedded-field name="status">
        <attribute name="value" column="status"/>
    </embedded-field>
    <field name="image" column="image"/>
    <field name="publishedOn" column="published_on"/>
</entity>